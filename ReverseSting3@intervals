#reverse at every k interval
#hop by 2*k get get to skip k intervals
class Solution(object):
    def reverseStr(self, s,k):
        """
        :type s: str
        
        :rtype: str
        """
        a = list(s)
        
        for i in range (0,len(a),2*k):
                (a[i:i+k])=reversed(a[i:i+k])
                print((a[i:i+k]))  
        return"".join(a)
